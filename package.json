{
  "name": "gatsby-p5-starter",
  "description": "A simple starter to get up and developing quickly with Gatsby and P5.js",
  "version": "0.1.0",
  "author": "shvembldr",
  "dependencies": {
    "gatsby": "^2.21.21",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-layout": "1.3.1",
    "gatsby-source-filesystem": "^2.3.1",
    "lodash": "4.17.21",
    "p5": "1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "babel-eslint": "10.1.0",
    "eslint": "7.0.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "plop": "2.6.0",
    "pre-commit": "1.2.2",
    "prettier": "2.0.5"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "pre-commit": "lint:staged",
  "keywords": [
    "gatsby",
    "p5.js",
    "creative coding"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint:fix": "eslint src/*.js --fix",
    "lint": "eslint src/*.js",
    "lint:staged": "lint-staged",
    "generate": "plop --plopfile generators/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shvembldr/gatsby-p5-starter"
  }
}
